{
  "name": "3dviewer",
  "version": "1.4.9",
  "private": true,
  "description": "A ThreeJS STL viewer",
  "main": "dist/viewer",
  "scripts": {
    "lint": "eslint src/viewer gulp gulpfile.babel.js",
    "develop": "parallelshell 'gulp dev' 'webpack-dev-server --content-base ./build'",
    "release": "npm run lint && gulp release",
    "deploy": "gulp deploy",
    "tag": "gulp tag",
    "dist": "git push origin && git push all3dp && git push origin --tags && git push all3dp --tags"
  },
  "engines": {
    "node": ">= 0.10",
    "npm": ">= 2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piscis/3dviewer"
  },
  "bugs": {
    "url": "https://github.com/piscis/3dviewer/issues"
  },
  "keywords": [
    "ThreeJS",
    "WebGL",
    "STL",
    "Viewer",
    "3D"
  ],
  "author": "Alexander Pirsig <self@pirsig.net>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "del": "^2.0.2",
    "eslint": "^1.5.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-bump": "^1.0.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-filter": "^3.0.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-git": "^1.6.0",
    "gulp-help": "^1.6.1",
