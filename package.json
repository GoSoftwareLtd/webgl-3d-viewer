    "gulp-load-plugins": "^0.10.0",
    "gulp-prompt": "^0.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "minimist": "^1.2.0",
